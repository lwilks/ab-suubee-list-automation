{
  "name": "ab-subee-list-automation",
  "productName": "ab-subee-list-automation",
  "version": "1.0.13",
  "description": "AmiBroker-Suubee List Automation",
  "main": "src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lwilks/ab-suubee-list-automation.git"
  },
  "keywords": [],
  "author": {
    "name": "Liam Wilks",
    "email": "liam.wilks@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ab_subee_list_automation"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "lwilks",
              "name": "ab-suubee-list-automation"
            },
            "prerelease": true
          }
        }
      ]
    }
  },
  "dependencies": {
    "axios": "^0.19.2",
    "csv-parse": "^4.10.1",
    "delay": "^4.3.0",
    "electron-log": "^4.2.2",
    "electron-progressbar": "^1.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "glob": "^7.1.6",
    "node-ig-api": "github:lwilks/node-ig-api",
    "p-retry": "^4.2.0",
    "request": "^2.88.2",
    "update-electron-app": "^1.5.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.52",
    "@electron-forge/maker-deb": "6.0.0-beta.52",
    "@electron-forge/maker-rpm": "6.0.0-beta.52",
    "@electron-forge/maker-squirrel": "6.0.0-beta.52",
    "@electron-forge/maker-zip": "6.0.0-beta.52",
    "@electron-forge/publisher-github": "^6.0.0-beta.52",
    "electron": "13.6.6",
    "electron-reload": "^1.5.0"
  }
}
